{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(ts|js|tsx)$": "ts-jest"
  },
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1",
    "^~/(.*)$": "<rootDir>/src/$1"
  },
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/",
    "/fixtures/"
  ]
}